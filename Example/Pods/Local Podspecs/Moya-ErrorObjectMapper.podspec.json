{
  "name": "Moya-ErrorObjectMapper",
  "version": "0.1.0",
  "summary": "ObjectMapper bindings when Moya encounters a status code error.",
  "description": "When Moya throws a status code MoyaError indicating that the API server returned an invalid status code, use Moya-ErrorObjectMapper to get error message from JSON body of error response.",
  "homepage": "https://github.com/levibostian/Moya-ErrorObjectMapper",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Levi Bostian": "levi.bostian@gmail.com"
  },
  "source": {
    "git": "https://github.com/Levi Bostian/Moya-ErrorObjectMapper.git",
    "tag": "0.1.0"
  },
  "social_media_url": "https://twitter.com/levibostian",
  "platforms": {
    "ios": "8.0"
  },
  "default_subspecs": "Core",
  "frameworks": "Foundation",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Moya-ErrorObjectMapper/Classes/Core/**/*.swift",
      "dependencies": {
        "Moya-ObjectMapper": [
          "~> 2.3.2"
        ]
      }
    },
    {
      "name": "RxSwift",
      "source_files": "Moya-ErrorObjectMapper/Classes/RxSwift/**/*.swift",
      "dependencies": {
        "Moya-ObjectMapper/RxSwift": [
          "~> 2.3.2"
        ],
        "Moya-ErrorObjectMapper/Core": [

        ]
      }
    }
  ]
}
